        document.addEventListener('keydown', function(e) {
            var forbiddenKeys = ["a", "s", "c", "x", "u"];
            var isCtrl = e.ctrlKey || e.metaKey; // Check for Ctrl (Windows) or Command (Mac)

            if (isCtrl && forbiddenKeys.includes(e.key.toLowerCase())) {
                e.preventDefault(); // Disable the forbidden key combinations
                alert('This action is disabled!'); // Optional: alert the user
            }
        });
    </script>


