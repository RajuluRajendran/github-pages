    <script> //timer new
        var spd = 1000;
        var spdVal = 1;
        var initialTime = 47 * 3600 + 16 * 60 + 59;
        var cntDown = initialTime * spdVal;

        function updateTimer() {
            var hr, mn, sc;
            cntDown--;

            if (cntDown < 0) { return false; }

            hr = Math.floor((cntDown / spdVal) / 3600);
            hr = (hr < 10 ? '0' + hr : hr);
            mn = Math.floor(((cntDown / spdVal) % 3600) / 60);
            mn = (mn < 10 ? '0' + mn : mn);
            sc = Math.floor((cntDown / spdVal) % 60);
            sc = (sc < 10 ? '0' + sc : sc);

            document.getElementById('hour').innerHTML = hr + "<br><span>Hours</span>";
            document.getElementById('minute').innerHTML = mn + "<br><span>Minutes</span>";
            document.getElementById('sec').innerHTML = sc + "<br><span>Seconds</span>";

            document.getElementById('hourU').innerHTML = hr + "<br><span>Hours</span>";
            document.getElementById('minuteU').innerHTML = mn + "<br><span>Minutes</span>";
            document.getElementById('secU').innerHTML = sc + "<br><span>Seconds</span>";
        }

        setInterval(updateTimer, spd);
    </script>
    <script> //count down - banner section
        function startCountdown() {
        var count = 27; // Starting value
        var endValue = 13; // Ending value
        var interval = 8000; // Interval in milliseconds (8 seconds)

        document.getElementById('countdown').innerText = count;

        function updateCountdown() {
            count--;

            document.getElementById('countdown').innerText = count;

            if (count <= endValue) {
            clearInterval(countdownInterval);
            }
        }

        var countdownInterval = setInterval(updateCountdown, interval);
        }

        setTimeout(startCountdown, 1000); // 1000 milliseconds = 1 second
    </script>
    <script> //count down - understand section 
        function startCountdown() {
        var count = 27; // Starting value
        var endValue = 13; // Ending value
        var interval = 8000; // Interval in milliseconds (8 seconds)

        document.getElementById('countdown2').innerText = count;

        function updateCountdown() {
            count--;

            document.getElementById('countdown2').innerText = count;

            if (count <= endValue) {
            clearInterval(countdownInterval);
            }
        }

        var countdownInterval = setInterval(updateCountdown, interval);
        }

        setTimeout(startCountdown, 1000); // 1000 milliseconds = 1 second
    </script>


<!-- JavaScript -->
<script>
    function getUtms() {
        let url = window.location.href;
        let params = {};
        let parser = document.createElement('a');
        parser.href = url;
        let query = parser.search.substring(1);
        let vars = query.split('&');
        
        for (let i = 0; i < vars.length; i++) {
            let pair = vars[i].split('=');
            params[pair[0]] = decodeURIComponent(pair[1]);
        }

        for (param in params) {
            let formField = document.getElementById(param);
            if (params[param] !== undefined && formField !== null) {
                formField.value = params[param];
            }
        }
    }

    // Call getUtms when the page loads
    window.onload = getUtms;

    // Rest of your code...
</script>


